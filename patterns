 class functionss{
 static boolean checkarmstrong(int n){
    int d=countnumbers(n);
    int sum=0;
    int temp=n;
    while(n!=0){
        sum=  Math.pow((n%10),d);
        n/=10;
    }
    return(sum==temp);
    }
    public static void main(String[] args) {
        System.out.print (checkarmstrong(12));
    }
}
    
    
