public class linearsearch{
    static boolean search(int[]array,int key){
        for(int i=0;i<=n;i++){
            if(i==key){
                return true;
                return false;
            }
        }
    }
        public static void main(String[]args){
            int[]array={1,2,3,4,5};
            int key=4;
            search(int[]array,int key);
        }
        
